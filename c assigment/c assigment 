display this information using printf
1.Dob
2.Name
3.address
4.your age.


#include<stdio.h>

int main()
{
    printf("your name : abc \n");
    printf("your dob : 09/08/2010\n");
    printf("your address :ahmedabad\n");
    printf("your age :20\n");

    return 0;

}


Write a program to make Simple calculator (to make addition, subtraction, multiplication, division).

#include<stdio.h>
int main()
{
    int a,b,add,sub,multi,divide;

    printf("enter the number :");
    scanf("%d",&a);


    printf("enter the second number :");
    scanf("%d",&b);

    add=a+b;
    sub=a-b;
    multi=a*b;
    divide=a%b;

    printf("add=%d\n",add);
    printf("sub=%d\n",sub);
    printf("multi=%d\n",multi);
    printf("divide=%d\n",divide);

    return 0;
}



WAP to find area of circle, rectangle and triangle


//area of circle
#include<stdio.h>

int main()
{
    float r;

    printf("enter the number to find area =");
    scanf("%f",&r);
    r=3.14*r*r;
    printf("area of circle is =%f",r);

    return 0;
}

//area of rectangle

#include<stdio.h>

void main()

{
    int l,b,a;

    printf("enter the lenght :");
    scanf("%d",&l);

    printf("enter the breadth :");
    scanf("%d",&b);

    a=l*b;

    printf("area=%d",a);
}

//area of triangle

#include<stdio.h>

int main()
{
    float b,h,a;

    printf("enter the breadth :");
    scanf("%f",&b);

    printf("enter the hight :");
    scanf("%f",&h);

     a=(b*h)/2;

     printf("area of triangle = %f",a);

}

WAP to find simple interest.


#include<stdio.h>

int main()
{
    float p,r,t,si;

    printf("enter the principle :");
    scanf("%f",&p);
    printf("enter the rate :");
    scanf("%f",&r);
    printf("enter the time :");
    scanf("%f",&t);

    si=(p*r*t)/100;

    printf("simple intrest is = %f",si);
}


WAP to check if the given year is a leap year or not. 

#include<stdio.h>
int main()
{
    int year;

    printf("enter the year");
    scanf("%d",&year);

    if (year % 400 == 0);
    {
        printf("%d is a leap year",year);
    }

    else if (year % 100 == 0);
    {
        printf("%d is not a leap year",year);
    }
    else if (year % 4 == 0);
    {
        printf("%d is a leap year",year);
    }

    else 
    {
        printf("%d is not a leap year",year);
    }
  return 0;
}


WAP to convert years into days and days into years.


#include<stdio.h>

int main()

{
    int days,years,y;

    printf("enter the number of years :");
    scanf("%d",&years);

    y=365*years;

    printf("No of days :%d",y);
    return 0;

}





(C Language Programing with C)


#include<stdio.h>
main()
{
	int a,b,ans,n;
	printf("enter a=");
	scanf("%d",&a);
	printf("enter b=");
	scanf("%d",&b);
	
	printf("\nSelect the operation to perform:\n\n");
	printf("\n1.Addition \n2.Subtration \n3.Multiplication \n4.Division \n\n");	
	scanf("%d",&n);
	
	switch(n)
	{
		case 1:
			ans=a+b;
			printf("\nAddition=%d",ans);
			break;
		case 2:
			ans=a-b;
			printf("\nSubtration=%d",ans);
			break;
		case 3:
		    ans=a*b;
		    printf("\nMultiplication=%d",ans);
		    break;
		case 4:
			ans=a%b;
			printf("\nDivision=%d",ans);
			break;
		default:
			printf("\nINVALID OUTPUT");
		
	}
}



WAP to swap two numbers without using third variable.

#include<stdio.h>
#include<conio.h>
void main()
{
	int a=10,b=20;
	printf("before swap a=%d b=%d",a,b);
	a=a+b;
	b=a-b;
	a=a-b;
	printf("\nafter swap a=%d b=%d",a,b);
}



WAP to find number is even or odd using ternary operator.

#include<stdio.h>
#include<conio.h>
void main()
{
	int n;
	printf("enter the number=");
	scanf("%d",&n);
	
	(n%2==0) ?
	(printf("%d is even number\n",n)):
	(printf("%d is odd number\n",n));
}


WAP to show
1. Monday to Sunday using switch case




#include<stdio.h>
#include<conio.h>
void main()
{
	int day;
	printf("\n");
	printf("\n1=Monday");
	printf("\n2=Tuesday");
	printf("\n3=Wednesday");
	printf("\n4=Thursday");
	printf("\n5=Friday");
	printf("\n6=Saturday");
	printf("\n7=Sunday");
	printf("\nEnter a day=");
	scanf("%d",&day);
	
	switch(day)
	{
		case 1:
			printf("Monday");
			break;
		case 2:
			printf("Tuesday");
			break;
		case 3:
			printf("Wednesday");
			break;
		case 4:
			printf("Thursday");
			break;
		case 5:
			printf("Friday");
			break;
		case 6:
			printf("Saturday");
			break;
		case 7:
			printf("Sunday");
			break;
		default:
			printf("invalid input");
			break;
	}
}

2. Vowel or Consonant using switch case 

#include<stdio.h>
#include<conio.h>
void main()
{
	char ch;
	printf("Enter any alphabets=");
	scanf("%c",&ch);
	switch(ch)
	{
		case 'a':
			printf("Vowel");
			break;
		case 'e':
			printf("Vowel");
			break;
		case 'i':
			printf("Vowel");
			break;
		case 'o':
			printf("Vowel");
			break;
		case 'u':
			printf("Vowel");
			break;
		case 'A':
			printf("Vowel");
			break;
		case 'E':
			printf("Vowel");
			break;
		case 'I':
			printf("Vowel");
			break;
		case 'O':
			printf("Vowel");
			break;
		case 'U':
			printf("Vowel");
			break;
		default:
			printf("consonants");
			break;
	}
}


Looping programs: 

1. WAP to print 972 to 897 using for loop.

#include<stdio.h>
#include<conio.h>
void main()
{
	int n,i;
	for(i=972;i>=897;i--)
	{
	printf("\n%d",i);
	}
	
}

2. WAP to take 10 no. Input from user and find out â€¦
How many Even numbers are there
How many odd numbers are there

#include<stdio.h>
#include<conio.h>
void main()
{
	int n;
	printf("enter the number=");
	scanf("%d",&n);
	
	(n%2==0) ?
	(printf("%d is even number\n",n)):
	(printf("%d is odd number\n",n));
}




WAP to print factorial of given number


#include<stdio.h>
#include<conio.h>
void main()
{
	int i,fact=1,n;
	printf("Enter the number whose factorial you want=");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		fact=fact*i;
	}
	printf("Factorial of %d is=%d",n,fact);
}


WAP to print Fibonacci series up to given numbers 


#include<stdio.h>
#include<conio.h>
void main()
{
	int n1=0,n2=1,n3,i,n;
	printf("Enter the number of elements=");
	scanf("%d",&n);
	printf("\n %d %d",n1,n2);
	for(i=2;i<n;i++)
	{
		n3=n1+n2;
		printf("\t %d",n3);
		n1=n2;
		n2=n3;
	}
}



Write a program make a summation of given number.


#include<stdio.h>
#include<conio.h>
void main()
{
	int n,sum=0,m;
	printf("enter the number=");
	scanf("%d",&n);
	
	while(n>0)
	{
		m=n%10;
		sum=sum+m;
		n=n/10;
	}
	printf("Sum is =%d",sum);
}



Write a program make a summation of given number.

#include<stdio.h>
#include<conio.h>
void main()
{
	int n,sum=0,f,l;
	printf("Enter any number to find sum of first and last digit=");
	scanf("%d",&n);
	
	l=n%10;
	f=n;
	
	while(n>=10)
	{
		n=n/10;
	}
	f=n;
	sum=f+l;
	printf("sum of first and last digit=%d",sum);
}



Patterns:_________________


#include<stdio.h>
#include<conio.h>
void main()
{
	int i,j,count=1;
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=i;j++)
		{
			printf(" ");
			printf("%d",count);
			count++;
		}
		printf("\n");
	}
	
}







---------




#include<stdio.h>
main()
{
	int i,j,k;
	for(i=1;i<=5;i++)
	{
		for(j=5;j>=i;j--)
		{
			printf(" ");
		}
		for(k=1;k<=i;k++)
		{
			printf("* ");
		}
		
		printf("\n");
	}

}




------------





#include<stdio.h>
main()
{
		int i,j;
	for(j=1;j<=6;j++)
	{
		for(i=1;i<=j;i++)
		{
			printf("*");
		}
		printf("\n");
	}
	for(j=1;j<=5;j++)
	{
		for(i=5;i>=j;i--)
		{
			printf("*");
		}
		printf("\n");
	}
	
}


-----------




#include<stdio.h>
#include<conio.h>
void main()
{
	int i,j,count='A';
	for(i='A';i<='E';i++)
	{
		for(j='A';j<=i;j++)
		{
			printf(" ");
			printf("%c",count);
			count++;
		}
		printf("\n");
	}
	
}



------------


#include<stdio.h>
#include<conio.h>
void main()
{
	int i,j,rows;
	printf("enter the number of rows=");
	scanf("%d",&rows);
	for(i=1;i<=rows;i++)
	{
		for(j=1;j<=i;j++)
		{
			printf(" ");
			printf("%c",'A' + j-1);
		}
		printf("\n");
	}
	
}